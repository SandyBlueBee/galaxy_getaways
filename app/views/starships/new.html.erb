

<h1>New Starship</h1>

<%= simple_form_for(@starship) do |f| %>
  <%= f.input :name %>
  <%= f.input :model %>
  <%= f.input :starship_class %>
  <%= f.input :manufacturer %>
  <%= f.input :length,  as: :integer %>
  <%= f.input :passangers,  as: :integer %>
  <%= f.input :max_speed, as: :integer %>
  <%= f.input :hyperdrive_rating , as: :integer %>
  <%= f.input :price, as: :integer %>
  <%= f.input :location %>
  <%= f.input :photos, as: :file, input_html: { multiple: true }  %>
  <%= f.submit %>
<% end %>

<%= link_to "Back", starships_path %>
